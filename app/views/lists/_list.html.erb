<%= div_for(list) do %>
  <%= link_to list_path(list) do %>
    <h4><%= list.name %>
      <% if list.shared_with != "" %>
       (Shared)
      <% end %>
    <% end %>
   <small>( <%= list.items.count %> Items, <%= link_to "Edit", edit_list_path(list) %> | <%= link_to "Delete", [list], method: :delete, remote: true %> )</small></h4>
<% end %>
